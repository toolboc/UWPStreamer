<?xml version="1.0" encoding="utf-8"?>
<Weavers>
  <Costura>
    <Target
      AfterTargets="AfterBuild;NonWinFodyTarget"
      Name="CleanReferenceCopyLocalPaths" >
      <Delete Files="@(ReferenceCopyLocalPaths->'$(OutDir)%(DestinationSubDirectory)%(Filename)%(Extension)')" />
    </Target>
  </Costura>
</Weavers>